#include<iostream>
#include<queue>
using namespace std;
int main()
{
    deque<int> d;
    d.push_front(1);
    d.push_back(2);
    cout<<d.front()<<endl; //front -< insertion 
    cout<<d.back()<<endl; //rear -< insertion
    d.pop_front();
 cout<<d.front()<<endl; //front -< insertion 
    cout<<d.back()<<endl;

    d.pop_back();
 
    return 0;
}

output:
1
2
2
2
